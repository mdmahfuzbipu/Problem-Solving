#include <stdio.h>

int main() {
    // Declaration and initialization of an array
    int numbers[5] = {1, 2, 3, 4, 5};

    // Accessing and printing array elements
    printf("Array elements: ");
    for (int i = 0; i < 5; i++) {
        printf("%d ", numbers[i]);
    }
    printf("\n");

    // Modifying array elements
    numbers[2] = 10;

    // Printing modified array
    printf("Modified array: ");
    for (int i = 0; i < 5; i++) {
        printf("%d ", numbers[i]);
    }
    printf("\n");

    return 0;
}
